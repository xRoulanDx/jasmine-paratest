{
	"name": "jasmine-paratest",
	"version": "0.0.8",
	"description": "Parametrize your `it` assertions.",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"********** Linters": "",
		"lint": "run-p prettier lint:ts",
		"prettier": "prettier --list-different \"src/**/*.ts\"",
		"lint:ts": "tslint -p tsconfig.json",
		"********** Test": "",
		"testOld": "jasmine-ts --config=jasmine.json",
		"test": "mocha -r ts-node/register src/**/paratest.spec.ts",
		"coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
		"********** Build": "",
		"build": "tsc",
		"********** Others": "",
		"prepare": "npm run build",
		"postversion": "git push && git push --tags"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xRoulanDx/jasmine-paratest.git"
	},
	"keywords": [
		"jasmine",
		"test",
		"parameterized",
		"unit"
	],
	"author": "xRoulanDx",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/xRoulanDx/jasmine-paratest/issues"
	},
	"homepage": "https://github.com/xRoulanDx/jasmine-paratest#readme",
	"devDependencies": {
		"@types/chai": "^4.1.7",
		"@types/jasmine": "^2.8.12",
		"@types/mocha": "^5.2.5",
		"@types/node": "^10.12.11",
		"chai": "^4.2.0",
		"coveralls": "^3.0.2",
		"husky": "^1.2.0",
		"jasmine": "^2.99.0",
		"jasmine-ts": "^0.3.0",
		"mocha": "^5.2.0",
		"npm-run-all": "^4.1.5",
		"nyc": "^13.1.0",
		"prettier": "^1.15.3",
		"ts-node": "^7.0.1",
		"tslint": "^5.11.0",
		"tslint-config-prettier": "^1.17.0",
		"typescript": "^3.2.2"
	},
	"peerDependencies": {
		"jasmine": ">2.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	},
	"dependencies": {
		"ts-mockito": "^2.3.1"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"exclude": [
			"**/*.d.ts"
		],
		"reporter": [
			"lcov"
		],
		"all": true
	}
}
