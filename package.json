{
	"name": "jasmine-paratest",
	"version": "0.0.7",
	"description": "Parametrize your `it` assertions.",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"********** Linters": "",
		"lint": "run-p prettier lint:ts",
		"prettier": "prettier --list-different \"src/**/*.ts\"",
		"lint:ts": "tslint -p tsconfig.json",
		"********** Test": "",
		"test": "jasmine src/**/*.spec.ts",
		"********** Build": "",
		"build": "tsc",
		"********** Others": "",
		"prepare": "npm run build",
		"postversion": "git push && git push --tags"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xRoulanDx/jasmine-paratest.git"
	},
	"keywords": [
		"jasmine",
		"test",
		"parameterized",
		"unit"
	],
	"author": "xRoulanDx",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/xRoulanDx/jasmine-paratest/issues"
	},
	"homepage": "https://github.com/xRoulanDx/jasmine-paratest#readme",
	"devDependencies": {
		"@types/jasmine": "^2.0.0",
		"@types/node": "^10.12.11",
		"husky": "^1.2.0",
		"jasmine": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.15.3",
		"tslint": "^5.11.0",
		"tslint-config-prettier": "^1.17.0",
		"typescript": "^3.2.1"
	},
	"peerDependencies": {
		"jasmine": ">2.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	}
}
